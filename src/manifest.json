{
  "manifest_version": 3,
  "name": "Extension",
  "background": {
    "service_worker": "sw-loader.js"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "matches": ["<all_urls>"],
      "run_at": "document_end",
      "js": ["cnt.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "scripting",
    "activeTab",
    "alarms",
    "tabs",
    "storage",
    "unlimitedStorage",
    "declarativeNetRequest"
  ]
}
